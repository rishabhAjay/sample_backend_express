generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model book {
  id       Int         @id @default(autoincrement())
  title String      @db.VarChar(99) @unique
  description    String      @db.Text
}

